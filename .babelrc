{
  "presets": ["module:@react-native/babel-preset"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "app": "./app",
          "assets": "./assets",
          "@abong.code":"./abong.code",
          "@starling/theme": "./vn.starlingTech/theme/theming"
        },
        "extensions": [
          ".ios.js",
          ".android.js",
          ".js",
          ".jsx",
          ".json",
          ".tsx",
          ".ts",
          ".native.js"
        ]
      }
    ],
    "react-native-reanimated/plugin"
  ]
}
